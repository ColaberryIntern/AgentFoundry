      "title": "Drift Detection Alerts",
      "description": "Error rate from the model server that may indicate model drift. A sustained increase above threshold warrants investigation.",
      "type": "timeseries",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "sum(rate(http_requests_total{job=\"model-server\",status_code=~\"5..\"}[5m])) / sum(rate(http_requests_total{job=\"model-server\"}[5m]))",
          "legendFormat": "Model Error Rate",
          "refId": "A"
        },
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket{job=\"model-server\"}[5m])) by (le)) / histogram_quantile(0.50, sum(rate(http_request_duration_seconds_bucket{job=\"model-server\"}[5m])) by (le))",
          "legendFormat": "Latency P99/P50 Ratio",
          "refId": "B"
        }
      ]
    }
